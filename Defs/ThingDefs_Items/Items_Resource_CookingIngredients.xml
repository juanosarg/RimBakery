<?xml version="1.0" encoding="utf-8" ?>

<Defs>


    <ThingDef Abstract="True" ParentName="ResourceBase" Name="RBK_IntermediateProduct">
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>5</DeteriorationRate>
 	<Mass>0.05</Mass>
    </statBases>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
    <tickerType>Rare</tickerType>
	
  </ThingDef>


  <ThingDef ParentName="RBK_IntermediateProduct">
    <defName>RBK_Sugar</defName>
    <label>sugar</label>
    <description>Refined white sugar extracted from sugar beets.</description>
   <graphicData>
      <texPath>Things/Item/RBK_Sugar</texPath>
    </graphicData>
    <statBases>
      <MarketValue>4</MarketValue>
    </statBases>
   
  </ThingDef>


<ThingDef ParentName="RBK_IntermediateProduct">
    <defName>RBK_WheatFlour</defName>
    <label>wheat flour</label>
    <description>Refined wheat flour, used in baking. Wheat flour takes a very long time to go bad, if stored in a dry environment.</description>
    <graphicData>
      <texPath>Things/Item/RBK_WheatFlour</texPath>
    </graphicData>
    <statBases>
      <MarketValue>4</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>100</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>



  </Defs>
